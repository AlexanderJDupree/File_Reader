# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  File_Reader_config = debug
  Tests_config = debug
  Example_config = debug
endif
ifeq ($(config),release)
  File_Reader_config = release
  Tests_config = release
  Example_config = release
endif

PROJECTS := File_Reader Tests Example

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

File_Reader:
ifneq (,$(File_Reader_config))
	@echo "==== Building File_Reader ($(File_Reader_config)) ===="
	@${MAKE} --no-print-directory -C . -f File_Reader.make config=$(File_Reader_config)
endif

Tests: File_Reader
ifneq (,$(Tests_config))
	@echo "==== Building Tests ($(Tests_config)) ===="
	@${MAKE} --no-print-directory -C . -f Tests.make config=$(Tests_config)
endif

Example: File_Reader
ifneq (,$(Example_config))
	@echo "==== Building Example ($(Example_config)) ===="
	@${MAKE} --no-print-directory -C . -f Example.make config=$(Example_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f File_Reader.make clean
	@${MAKE} --no-print-directory -C . -f Tests.make clean
	@${MAKE} --no-print-directory -C . -f Example.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   File_Reader"
	@echo "   Tests"
	@echo "   Example"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"