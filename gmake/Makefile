endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_posix_x64)
  FileReader_config = debug_posix_x64
  Tests_config = debug_posix_x64
  Example_config = debug_posix_x64
endif
ifeq ($(config),debug_win64)
  FileReader_config = debug_win64
  Tests_config = debug_win64
  Example_config = debug_win64
endif
ifeq ($(config),release_posix_x64)
  FileReader_config = release_posix_x64
  Tests_config = release_posix_x64
  Example_config = release_posix_x64
endif
ifeq ($(config),release_win64)
  FileReader_config = release_win64
  Tests_config = release_win64
  Example_config = release_win64
endif

PROJECTS := FileReader Tests Example

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

FileReader:
ifneq (,$(FileReader_config))
	@echo "==== Building FileReader ($(FileReader_config)) ===="
	@${MAKE} --no-print-directory -C . -f FileReader.make config=$(FileReader_config)
endif

Tests: FileReader
ifneq (,$(Tests_config))
	@echo "==== Building Tests ($(Tests_config)) ===="
	@${MAKE} --no-print-directory -C . -f Tests.make config=$(Tests_config)
endif

Example: FileReader
ifneq (,$(Example_config))
	@echo "==== Building Example ($(Example_config)) ===="
	@${MAKE} --no-print-directory -C . -f Example.make config=$(Example_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f FileReader.make clean
	@${MAKE} --no-print-directory -C . -f Tests.make clean
	@${MAKE} --no-print-directory -C . -f Example.make clean

